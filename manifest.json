{
  "manifest_version": 3,
  "name": "Bypass City",
  "version": "1.0",
  "description": "Unofficial extension that sends a link to bypass city",
  "homepage_url": "https://github.com/arushenst/bypass-city",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}